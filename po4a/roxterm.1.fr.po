#. type: Content of: <refentry><refsect1><title>
#: ../roxterm.1.xml.in:112
msgid "DESCRIPTION"
msgstr "DESCRIPTION"

#. type: Content of: <refentry><refsect1><para>
#: ../roxterm.1.xml.in:114
msgid ""
"This manual page documents briefly the <command>&dhpackage;</command> "
"command."
msgstr ""
"Cette page de manuel documente brièvement la commande <command> &;dhpackage;"
" </command>."

#. type: Content of: <refentry><refsect1><para>
#: ../roxterm.1.xml.in:117
msgid "<command>&dhpackage;</command> is a terminal emulator."
msgstr "<command>&dhpackage;</command> est un émulateur de terminal."

#. type: Content of: <refentry><refsect1><title>
#: ../roxterm.1.xml.in:121
msgid "OPTIONS"
msgstr "OPTIONS"

#. type: Content of: <refentry><refsect1><para>
#: ../roxterm.1.xml.in:123
msgid ""
"These programs follow the usual &gnu; command line syntax, with long options"
" starting with two dashes (`-').  A summary of options is included below."
msgstr ""
"Ces programmes suivent la syntaxe habituelle &gnu; de la ligne de commande, avec des options longues commençant par deux tirets (`-'). \n"
"Un résumé des options est inclus ci-dessous."

#. type: Content of: <refentry><refsect1><variablelist><varlistentry><term>
#: ../roxterm.1.xml.in:129
msgid "<option>-?</option> <option>--help</option>"
msgstr "<option>-?</option> <option>--help</option>"

#. type: Content of:
#. <refentry><refsect1><variablelist><varlistentry><listitem><para>
#: ../roxterm.1.xml.in:133
msgid "Show description of options."
msgstr "Afficher la description des options."

#. type: Content of: <refentry><refsect1><variablelist><varlistentry><term>
#: ../roxterm.1.xml.in:137
msgid "<option>--help-all</option>"
msgstr "<option>--help-all</option>"

#. type: Content of:
#. <refentry><refsect1><variablelist><varlistentry><listitem><para>
#: ../roxterm.1.xml.in:140
msgid "Show all options."
msgstr "Afficher toutes les options."

#. type: Content of: <refentry><refsect1><variablelist><varlistentry><term>
#: ../roxterm.1.xml.in:144
msgid "<option>--help-gtk</option>"
msgstr "<option>--help-gtk</option>"

#. type: Content of:
#. <refentry><refsect1><variablelist><varlistentry><listitem><para>
#: ../roxterm.1.xml.in:147
msgid "Show all GTK+ options."
msgstr "Afficher toutes les options GTK +."

